<?xml version="1.0"?>
<config>
    <modules>
        <Onetree_Checkout>
            <version>0.0.1</version>
        </Onetree_Checkout>
    </modules>
    <global>
        <models>
            <otcheckout>
                <class>Onetree_Checkout_Model</class>
            </otcheckout>
        </models>
    </global>
    <frontend>
        <events>
            <checkout_cart_add_product_complete>
                <observers>
                    <otcheckout_addtocart_observer>
                        <type>singleton</type>
                        <class>Onetree_Checkout_Model_Observer</class>
                        <method>addtocart</method>
                    </otcheckout_addtocart_observer>
                </observers>
            </checkout_cart_add_product_complete>
        </events>
    </frontend>
</config>